<Window x:Name="HomePage" x:Class="Project_Genesis_Source.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Project_Genesis_Source"
        mc:Ignorable="d"
        Title="CAJN Software" Width="1200" Height="725" Icon="icon.png" ResizeMode="CanResizeWithGrip" WindowStartupLocation="CenterScreen" Topmost="True">
    <Window.Resources>
        <Style x:Key="FocusVisual">
            <Setter Property="Control.Template">
                <Setter.Value>
                    <ControlTemplate>
                        <Rectangle Margin="2" SnapsToDevicePixels="true" Stroke="{DynamicResource {x:Static SystemColors.ControlTextBrushKey}}" StrokeThickness="1" StrokeDashArray="1 2"/>
                    </ControlTemplate>
                </Setter.Value>
            </Setter>
        </Style>
        <SolidColorBrush x:Key="Button.Static.Background" Color="#FFDDDDDD"/>
        <SolidColorBrush x:Key="Button.Static.Border" Color="#FF707070"/>
        <SolidColorBrush x:Key="Button.MouseOver.Background" Color="#FFBEE6FD"/>
        <SolidColorBrush x:Key="Button.MouseOver.Border" Color="#FF3C7FB1"/>
        <SolidColorBrush x:Key="Button.Pressed.Background" Color="#FFC4E5F6"/>
        <SolidColorBrush x:Key="Button.Pressed.Border" Color="#FF2C628B"/>
        <SolidColorBrush x:Key="Button.Disabled.Background" Color="#FFF4F4F4"/>
        <SolidColorBrush x:Key="Button.Disabled.Border" Color="#FFADB2B5"/>
        <SolidColorBrush x:Key="Button.Disabled.Foreground" Color="#FF838383"/>
        <Style x:Key="ButtonRoundEdges" TargetType="{x:Type Button}">
            <Setter Property="FocusVisualStyle" Value="{StaticResource FocusVisual}"/>
            <Setter Property="Background" Value="{StaticResource Button.Static.Background}"/>
            <Setter Property="BorderBrush" Value="{StaticResource Button.Static.Border}"/>
            <Setter Property="Foreground" Value="{DynamicResource {x:Static SystemColors.ControlTextBrushKey}}"/>
            <Setter Property="BorderThickness" Value="1"/>
            <Setter Property="HorizontalContentAlignment" Value="Center"/>
            <Setter Property="VerticalContentAlignment" Value="Center"/>
            <Setter Property="Padding" Value="1"/>
            <Setter Property="Template">
                <Setter.Value>
                    <ControlTemplate TargetType="{x:Type Button}">
                        <Grid x:Name="border" SnapsToDevicePixels="true">
                            <Rectangle HorizontalAlignment="Stretch" Height="Auto" Margin="21,15,-20.333,0" VerticalAlignment="Stretch" Width="Auto" RadiusY="16.034" RadiusX="16.034" StrokeThickness="2" Stroke="{x:Null}">
                                <Rectangle.Fill>
                                    <LinearGradientBrush EndPoint="0.5,1" MappingMode="RelativeToBoundingBox" StartPoint="0.5,0">
                                        <GradientStop Color="#FF75ACE4" Offset="0.5"/>
                                        <GradientStop Color="White" Offset="0.18"/>
                                        <GradientStop Color="White" Offset="0.828"/>
                                    </LinearGradientBrush>
                                </Rectangle.Fill>
                            </Rectangle>
                            <ContentPresenter x:Name="contentPresenter" Focusable="False" RecognizesAccessKey="True" SnapsToDevicePixels="{TemplateBinding SnapsToDevicePixels}" VerticalAlignment="Stretch" Margin="41.334,37.782,0,21.884" Height="Auto" Width="Auto" HorizontalAlignment="Stretch" FlowDirection="LeftToRight"/>
                        </Grid>
                        <ControlTemplate.Triggers>
                            <Trigger Property="IsDefaulted" Value="true"/>
                            <Trigger Property="IsMouseOver" Value="true"/>
                            <Trigger Property="IsPressed" Value="true"/>
                            <Trigger Property="IsEnabled" Value="false">
                                <Setter Property="TextElement.Foreground" TargetName="contentPresenter" Value="{StaticResource Button.Disabled.Foreground}"/>
                            </Trigger>
                        </ControlTemplate.Triggers>
                    </ControlTemplate>
                </Setter.Value>
            </Setter>
        </Style>
    </Window.Resources>
    <Window.Foreground>
        <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
            <GradientStop Color="Black" Offset="0"/>
            <GradientStop Color="#FF0047FF" Offset="1"/>
        </LinearGradientBrush>
    </Window.Foreground>
    <Window.Background>
        <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
            <GradientStop Color="Black" Offset="1"/>
            <GradientStop Color="#FF0047FF"/>
        </LinearGradientBrush>
    </Window.Background>
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition/>
        </Grid.RowDefinitions>

        <Grid.Background>
            <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
                <GradientStop Color="White" Offset="1"/>
                <GradientStop Color="#FF98C7F7"/>
                <GradientStop Color="#FF5795D3" Offset="0.5"/>
            </LinearGradientBrush>
        </Grid.Background>
        <Grid.ColumnDefinitions>
            <ColumnDefinition/>
            <ColumnDefinition Width="0*"/>
        </Grid.ColumnDefinitions>

        <Rectangle Fill="#FF9D9DCB" Margin="0,-1,0,0" Height="180" VerticalAlignment="Top"/>

        <Rectangle Fill="#FF9999AE" Margin="265,179,0,0" Opacity="0.5"/>

        <Image Height="120" Margin="15,10,0,0" VerticalAlignment="Top" Source="icon.png" Stretch="Fill" HorizontalAlignment="Left" Width="100"/>
        <Button x:Name="home1" Content="Home" Margin="10,179,0,0" Background="{x:Null}" Foreground="#FF0C1249" FontSize="36" FontFamily="PenultimateLightItal" FontWeight="Bold" Height="60" VerticalAlignment="Top" BorderBrush="{x:Null}" HorizontalAlignment="Left" Width="250" Click="home1_Click"/>

        <Button x:Name="createInv" Content="Create Invoice" Margin="10,244,0,0" BorderBrush="{x:Null}" Click="createInv_Click" Background="{x:Null}" Foreground="#FF0C1249" FontSize="36" FontFamily="PenultimateLightItal" FontWeight="Bold" Height="60" VerticalAlignment="Top" HorizontalAlignment="Left" Width="250">
            <!--   <Button.Foreground>
                <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
                    <GradientStop Color="Black" Offset="0"/>
                    <GradientStop Color="#FF0047FF" Offset="1"/>
                </LinearGradientBrush>
            </Button.Foreground> (Description: The objkect 'Button' already has achild and cannot add 'Button'. 'Button' can accept only one child) -->

        </Button>
        <Button x:Name="customerAdd" Content="Add Customer" Margin="10,309,0,0" BorderBrush="{x:Null}" Click="customerAdd_Click" Background="{x:Null}" Foreground="#FF0C1249" FontSize="36" FontFamily="PenultimateLightItal" FontWeight="Bold" Height="60" VerticalAlignment="Top" HorizontalAlignment="Left" Width="250"/>
        <Button x:Name="vehicleAdd" Content="Add Vehicle" Margin="10,374,0,0" BorderBrush="{x:Null}" Click="vehicleAdd_Click" Background="{x:Null}" Foreground="#FF0C1249" FontSize="36" FontFamily="PenultimateLightItal" FontWeight="Bold" Height="60" VerticalAlignment="Top" HorizontalAlignment="Left" Width="250"/>
        <Button x:Name="addPart" Content="Add Part" Margin="10,439,0,0" BorderBrush="{x:Null}" Click="addPart_Click" Background="{x:Null}" Foreground="#FF0C1249" FontSize="36" FontFamily="PenultimateLightItal" FontWeight="Bold" Height="60" VerticalAlignment="Top" HorizontalAlignment="Left" Width="250"/>
        <Button x:Name="clientManage" Content="Manage Clients" Margin="10,504,0,0" BorderBrush="{x:Null}" Click="clientManage_Click" Background="{x:Null}" Foreground="#FF0C1249" FontSize="36" FontFamily="PenultimateLightItal" FontWeight="Bold" Height="60" VerticalAlignment="Top" HorizontalAlignment="Left" Width="250"/>
        <Button x:Name="backupTab" Content="Backup/&#xA;Archive" Margin="10,569,0,0" BorderBrush="{x:Null}" Click="backupTab_Click" Background="{x:Null}" Foreground="#FF0C1249" FontSize="36" FontFamily="PenultimateLightItal" FontWeight="Bold" Height="90" VerticalAlignment="Top" HorizontalAlignment="Left" Width="250"/>
        <Label Content="CAJN " Margin="115,10,648.779,601" Foreground="#FF0C1249" FontSize="72" FontFamily="Splash"/>
        <Label Content="voice 2018 " Margin="115,32.917,612.201,548.083" FontFamily="HarvestItal" FontSize="65" TextOptions.TextHintingMode="Fixed" Background="{x:Null}" FontWeight="Bold" Foreground="#FF0C1249" RenderTransformOrigin="0.5,0.5">
            <Label.RenderTransform>
                <TransformGroup>
                    <ScaleTransform/>
                    <SkewTransform AngleY="-0.273"/>
                    <RotateTransform/>
                    <TranslateTransform Y="-1.495"/>
                </TransformGroup>
            </Label.RenderTransform>
        </Label>
        <Frame x:Name="Main" Margin="265,179,10,0" NavigationUIVisibility="Hidden" Foreground="{x:Null}" Cursor="Hand" Height="505" VerticalAlignment="Top" ClipToBounds="True" SnapsToDevicePixels="True">
            <Frame.Content>
                <Label Content="Welcome to your Personalized &#xD;&#xA;Invoice Generating Software&#xD;&#xA;Brought to you by CAJN Software&#xD;&#xA;Batteries not included..." Height="505" VerticalAlignment="Top" FontFamily="HarvestItal" FontSize="50" Margin="0,0,120,0"/>
            </Frame.Content>
        </Frame>
        <Path Data="M498,53 C981,54 1047,1 861,78 860.63993,78.149059 834.68458,146.86279 672.00006,75.999993 509,4.9997566 529.00001,125.00029 733.00008,94.000139 937.00015,62.99999 570.59805,263.75815 739.00008,86.0001 811.00011,9.9999227 485,32.999872 775.0001,54.99979 1065.0002,76.999707 1035.0002,0.99990661 804.00011,58.99972 573.00003,116.99953 924.00032,-69.999865 774.00021,66.999694" Fill="#FF0E0E47" Margin="498,15.634,87.125,0" Stretch="Fill" Height="144.748" VerticalAlignment="Top"/>

    </Grid>
</Window>
