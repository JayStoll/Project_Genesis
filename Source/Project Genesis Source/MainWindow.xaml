<Window x:Name="HomePage" x:Class="Project_Genesis_Source.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Project_Genesis_Source"
        mc:Ignorable="d"
        Title="CAJN Software" Width="1800" Height="1000" Icon="icon.png" ResizeMode="CanResizeWithGrip" WindowStartupLocation="CenterScreen" Topmost="True" WindowState="Maximized">
    <Window.Resources>
        <Style x:Key="FocusVisual">
            <Setter Property="Control.Template">
                <Setter.Value>
                    <ControlTemplate>
                        <Rectangle Margin="2" SnapsToDevicePixels="true" Stroke="{DynamicResource {x:Static SystemColors.ControlTextBrushKey}}" StrokeThickness="1" StrokeDashArray="1 2"/>
                    </ControlTemplate>
                </Setter.Value>
            </Setter>
        </Style>
        <SolidColorBrush x:Key="Button.Static.Background" Color="#FFDDDDDD"/>
        <SolidColorBrush x:Key="Button.Static.Border" Color="#FF707070"/>
        <SolidColorBrush x:Key="Button.MouseOver.Background" Color="#FFBEE6FD"/>
        <SolidColorBrush x:Key="Button.MouseOver.Border" Color="#FF3C7FB1"/>
        <SolidColorBrush x:Key="Button.Pressed.Background" Color="#FFC4E5F6"/>
        <SolidColorBrush x:Key="Button.Pressed.Border" Color="#FF2C628B"/>
        <SolidColorBrush x:Key="Button.Disabled.Background" Color="#FFF4F4F4"/>
        <SolidColorBrush x:Key="Button.Disabled.Border" Color="#FFADB2B5"/>
        <SolidColorBrush x:Key="Button.Disabled.Foreground" Color="#FF838383"/>
        <Style x:Key="ButtonRoundEdges" TargetType="{x:Type Button}">
            <Setter Property="FocusVisualStyle" Value="{StaticResource FocusVisual}"/>
            <Setter Property="Background" Value="{StaticResource Button.Static.Background}"/>
            <Setter Property="BorderBrush" Value="{StaticResource Button.Static.Border}"/>
            <Setter Property="Foreground" Value="{DynamicResource {x:Static SystemColors.ControlTextBrushKey}}"/>
            <Setter Property="BorderThickness" Value="1"/>
            <Setter Property="HorizontalContentAlignment" Value="Center"/>
            <Setter Property="VerticalContentAlignment" Value="Center"/>
            <Setter Property="Padding" Value="1"/>
            <Setter Property="Template">
                <Setter.Value>
                    <ControlTemplate TargetType="{x:Type Button}">
                        <Grid x:Name="border" SnapsToDevicePixels="true">
                            <Rectangle HorizontalAlignment="Stretch" Height="Auto" Margin="21,15,-20.333,0" VerticalAlignment="Stretch" Width="Auto" RadiusY="16.034" RadiusX="16.034" StrokeThickness="2" Stroke="{x:Null}">
                                <Rectangle.Fill>
                                    <LinearGradientBrush EndPoint="0.5,1" MappingMode="RelativeToBoundingBox" StartPoint="0.5,0">
                                        <GradientStop Color="#FF75ACE4" Offset="0.5"/>
                                        <GradientStop Color="White" Offset="0.18"/>
                                        <GradientStop Color="White" Offset="0.828"/>
                                    </LinearGradientBrush>
                                </Rectangle.Fill>
                            </Rectangle>
                            <ContentPresenter x:Name="contentPresenter" Focusable="False" RecognizesAccessKey="True" SnapsToDevicePixels="{TemplateBinding SnapsToDevicePixels}" VerticalAlignment="Stretch" Margin="41.334,37.782,0,21.884" Height="Auto" Width="Auto" HorizontalAlignment="Stretch" FlowDirection="LeftToRight"/>
                        </Grid>
                        <ControlTemplate.Triggers>
                            <Trigger Property="IsDefaulted" Value="true"/>
                            <Trigger Property="IsMouseOver" Value="true"/>
                            <Trigger Property="IsPressed" Value="true"/>
                            <Trigger Property="IsEnabled" Value="false">
                                <Setter Property="TextElement.Foreground" TargetName="contentPresenter" Value="{StaticResource Button.Disabled.Foreground}"/>
                            </Trigger>
                        </ControlTemplate.Triggers>
                    </ControlTemplate>
                </Setter.Value>
            </Setter>
        </Style>
    </Window.Resources>
    <Window.Foreground>
        <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
            <GradientStop Color="Black" Offset="0"/>
            <GradientStop Color="#FF0047FF" Offset="1"/>
        </LinearGradientBrush>
    </Window.Foreground>
    <Window.Background>
        <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
            <GradientStop Color="Black" Offset="1"/>
            <GradientStop Color="#FF0047FF"/>
        </LinearGradientBrush>
    </Window.Background>
    <Grid Background="#FF84CDFF">
        <Grid.RowDefinitions>
            <RowDefinition/>
        </Grid.RowDefinitions>
        <Grid.ColumnDefinitions>
            <ColumnDefinition/>
            <ColumnDefinition Width="0*"/>
        </Grid.ColumnDefinitions>

        <Image Height="160" Margin="15,10,0,0" VerticalAlignment="Top" Source="icon.png" Stretch="Fill" HorizontalAlignment="Left" Width="140"/>
        <Button x:Name="home1" Content="Home" Margin="15,180.04,0,0" Background="{x:Null}" Foreground="#FF0C1249" FontSize="40" FontFamily="Lithos Pro Regular" Height="100" VerticalAlignment="Top" BorderBrush="{x:Null}" HorizontalAlignment="Left" Width="330" Click="home1_Click"/>

        <Button x:Name="createInv" Content="Create Invoice" Margin="10,285.04,0,0" BorderBrush="{x:Null}" Click="createInv_Click" Background="{x:Null}" Foreground="#FF0C1249" FontSize="40" FontFamily="Lithos Pro Regular" Height="100" VerticalAlignment="Top" HorizontalAlignment="Left" Width="330">
            <!--   <Button.Foreground>
                <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
                    <GradientStop Color="Black" Offset="0"/>
                    <GradientStop Color="#FF0047FF" Offset="1"/>
                </LinearGradientBrush>
            </Button.Foreground> (Description: The objkect 'Button' already has achild and cannot add 'Button'. 'Button' can accept only one child) -->

        </Button>
        <Button x:Name="customerAdd" Content="Add Customer" Margin="10,390.04,0,0" BorderBrush="{x:Null}" Click="customerAdd_Click" Background="{x:Null}" Foreground="#FF0C1249" FontSize="40" FontFamily="Lithos Pro Regular" Height="100" VerticalAlignment="Top" HorizontalAlignment="Left" Width="330"/>
        <Button x:Name="vehicleAdd" Content="Add Vehicle" Margin="10,495.04,0,0" BorderBrush="{x:Null}" Click="vehicleAdd_Click" Background="{x:Null}" Foreground="#FF0C1249" FontSize="40" FontFamily="Lithos Pro Regular" Height="100" VerticalAlignment="Top" HorizontalAlignment="Left" Width="330"/>
        <Button x:Name="addPart" Content="Add Part" Margin="15,600.04,0,0" BorderBrush="{x:Null}" Click="addPart_Click" Background="{x:Null}" Foreground="#FF0C1249" FontSize="40" FontFamily="Lithos Pro Regular" Height="100" VerticalAlignment="Top" HorizontalAlignment="Left" Width="330"/>
        <Button x:Name="clientManage" Content="Manage Clients" Margin="10,705.04,0,0" BorderBrush="{x:Null}" Click="clientManage_Click" Background="{x:Null}" Foreground="#FF0C1249" FontSize="40" FontFamily="Lithos Pro Regular" Height="100" VerticalAlignment="Top" HorizontalAlignment="Left" Width="330"/>
        <Button x:Name="backupTab" Content="Backup/&#xA;Archive" Margin="10,810.04,0,0" BorderBrush="{x:Null}" Click="backupTab_Click" Background="{x:Null}" Foreground="#FF0C1249" FontSize="40" FontFamily="Lithos Pro Regular" Height="90" VerticalAlignment="Top" HorizontalAlignment="Left" Width="330"/>
        <Label Content="CAJNvoice -2018- " Margin="160,16.04,512.996,0" Foreground="#FF0C1249" FontSize="120" FontFamily="Splash" Height="164" VerticalAlignment="Top"/>
        <Frame x:Name="Main" Margin="350,179,10,0" NavigationUIVisibility="Hidden" Foreground="{x:Null}" Cursor="Hand" Height="781" VerticalAlignment="Top" ClipToBounds="True" SnapsToDevicePixels="True">
            <Frame.Content>
                <Grid Height="780" Margin="10,0.5,0,0.5">
                    <Rectangle HorizontalAlignment="Left" Height="248.945" Margin="4.5,71.147,0,0" VerticalAlignment="Top" Width="447.262" RadiusY="69.5" RadiusX="69.5" StrokeThickness="10">
                        <Rectangle.Stroke>
                            <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
                                <GradientStop Color="#FF3387CD" Offset="0"/>
