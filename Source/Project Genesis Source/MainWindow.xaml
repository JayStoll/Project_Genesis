                            </LinearGradientBrush>
                        </Rectangle.Stroke>
                    </Rectangle>




                    <Button x:Name="createInvB" Content="Create&#xA;Invoice" Margin="4.5,71.147,0,0" Background="{x:Null}" BorderBrush="{x:Null}" FontFamily="Lithos Pro Regular" Foreground="#FF0C1249" Height="248.945" VerticalAlignment="Top" HorizontalAlignment="Left" Width="447.262" FontSize="55" Click="createInvB_Click" ClickMode="Press"/>
                    <Button x:Name="addCustomerB" Content="Add Customer" Margin="485.815,71.147,0,0" Background="{x:Null}" BorderBrush="{x:Null}" FontFamily="Lithos Pro Regular" Foreground="#FF0C1249" Height="248.945" VerticalAlignment="Top" HorizontalAlignment="Left" Width="447.262" FontSize="55" Click="addCustomerB_Click" ClickMode="Press"/>
                    <Button x:Name="addPartB" Content="Add Parts" Margin="4.5,435.736,0,0" Background="{x:Null}" BorderBrush="{x:Null}" FontFamily="Lithos Pro Regular" Foreground="#FF0C1249" Height="248.945" VerticalAlignment="Top" HorizontalAlignment="Left" Width="447.262" FontSize="55" Click="addPartB_Click" ClickMode="Press"/>
                    <Button x:Name="ManageClientsB" Content="Manage&#xA;Clients" Margin="485.815,435.736,0,0" Background="{x:Null}" BorderBrush="{x:Null}" FontFamily="Lithos Pro Regular" Foreground="#FF0C1249" Height="248.945" VerticalAlignment="Top" HorizontalAlignment="Left" Width="447.262" FontSize="55" Click="ManageClientsB_Click" ClickMode="Press"/>
                    <Button x:Name="backupB" Content="Backup/&#xA;Archive" Margin="970.998,435.736,0,0" Background="{x:Null}" BorderBrush="{x:Null}" FontFamily="Lithos Pro Regular" Foreground="#FF0C1249" Height="248.945" VerticalAlignment="Top" HorizontalAlignment="Left" Width="447.263" FontSize="55" Click="backupB_Click" ClickMode="Press"/>
                    <Button x:Name="addVehicleB" Content="Add Vehicle" Margin="970.998,71.147,0,0" Background="{x:Null}" BorderBrush="{x:Null}" FontFamily="Lithos Pro Regular" Foreground="#FF0C1249" Height="248.945" VerticalAlignment="Top" HorizontalAlignment="Left" Width="447.263" FontSize="55" Click="addVehicleB_Click" ClickMode="Press"/>



                </Grid>

            </Frame.Content>
        </Frame>
        <Button x:Name="aboutUs" Content="Button" HorizontalAlignment="Left" Height="180.04" VerticalAlignment="Top" Width="160" Background="{x:Null}" BorderBrush="{x:Null}" Foreground="{x:Null}" Cursor="Hand" Click="aboutUs_Click"/>

    </Grid>
</Window>
