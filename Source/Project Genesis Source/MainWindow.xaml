                    </Rectangle>
                    <Rectangle HorizontalAlignment="Left" Height="248.945" Margin="4.5,435.736,0,0" VerticalAlignment="Top" Width="447.262" RadiusY="69.5" RadiusX="69.5" StrokeThickness="10">
                        <Rectangle.Stroke>
                            <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
                                <GradientStop Color="#FF3387CD" Offset="0"/>
                                <GradientStop Color="White" Offset="1"/>
                            </LinearGradientBrush>
                        </Rectangle.Stroke>
                    </Rectangle>
                    <Rectangle HorizontalAlignment="Left" Height="248.945" Margin="485.815,435.736,0,0" VerticalAlignment="Top" Width="447.262" RadiusY="69.5" RadiusX="69.5" StrokeThickness="10">
                        <Rectangle.Stroke>
                            <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
                                <GradientStop Color="#FF3387CD" Offset="0"/>
                                <GradientStop Color="White" Offset="1"/>
                            </LinearGradientBrush>
                        </Rectangle.Stroke>
                    </Rectangle>
                    <Rectangle HorizontalAlignment="Left" Height="248.945" Margin="970.998,435.736,0,0" VerticalAlignment="Top" Width="447.263" RadiusY="69.5" RadiusX="69.5" StrokeThickness="10">
                        <Rectangle.Stroke>
                            <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
                                <GradientStop Color="#FF3387CD" Offset="0"/>
